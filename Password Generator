import string
import random
size=int(input("enter size of password you want: "))
large=string.ascii_letters+string.digits+string.punctuation
chara=random.choice(string.ascii_letters)
digi=random.choice(string.digits)
punc=random.choice(string.punctuation)
temp=chara+digi+punc
for i in range(size-3):
  temp=temp+random.choice(large)
print("password is",temp)


